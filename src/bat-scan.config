##################
### leaf scans ###
##################

[architecture]
type        = leaf
module      = bat.checks
method      = scanArchitecture
noscan      = text:xml:graphics:pdf:compressed:audio:video:mp4:java:appledouble
description = Scan executable file architecture
enabled     = yes

[busybox-version]
